metamodel FacebookLanguage {
	Network {
		.description : EString
		<+>-fiendships(0..*)-> Friendship
		<+>-users(0..*)-> User
	}
	
	Friendship {
		.startFrom : EDate
		-friends(0..*)-> User
	}
	
	User {
		.name : EString
		-spouse(0..1)-> User
		-siblings(0..*)-> User
		-parents(0..*)-> User
	}
}

metamodel InstagramLanguage {
	Network {
		.description : EString
		<+>-users(0..*)-> User
	}
	
	User {
		.name : EString
		-follows(0..*)-> User
	}
}