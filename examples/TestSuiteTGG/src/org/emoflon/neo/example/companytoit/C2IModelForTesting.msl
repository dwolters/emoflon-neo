		.name : "AnyNetwork"
		-laptop->l1
	}
	
	l1 : Laptop{
		.name : "Laptop1"
	}
}

rule InConsistentTriple1{
	it : IT {
		.name : "Fraunhofer"
		++ -corr-> c{
			._type_ := "CompanyToIT"
		}
	}
	
	l1 : Laptop{
		.name : "Laptop1"
		++ -corr-> e1{
			._type_ := "EmployeeToLaptop"
		}
	}
	
	c: Company{
		.name : "Fraunhofer"
	}
	
	e1: Employee{
		.name : "Mario"
	}
}

