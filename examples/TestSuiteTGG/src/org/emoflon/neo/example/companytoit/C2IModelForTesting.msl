import "platform:/resource/CompanyToIT/src/metamodels/IT.msl"
import "platform:/resource/CompanyToIT/src/metamodels/Company.msl"

model ConsistentSource1{
	c: Company{
		.name : "Fraunhofer"
		-employee-> e1
		-employee-> e2
	}
	
	e1: Employee{
		.name : "Mario"
	}
	
	e2: Employee{
		.name : "Michael"
	}	
}

model ConsistentTarget1{
	it : IT {
		-network-> network
		.name : "Fraunhofer"
	}
	
	network : Network {
		.name : "AnyNetwork"
		-laptop->l1
		-laptop->l2
	}
	
	l1 : Laptop{
		.name : "Laptop1"
	}
	
	l2 : Laptop{
		.name : "Laptop2"
	}
}

rule ConsistentTriple1{
	it : IT {
		.name : "Fraunhofer"
		++ -corr-> c{
			._type_ := "CompanyToIT"
		}
	}
	
	l1 : Laptop{
		.name : "Laptop1"
		++ -corr-> e1{
			._type_ := "EmployeeToLaptop"
		}
	}
	
	l2 : Laptop{
		.name : "Laptop2"
		++ -corr-> e2{
			._type_ := "EmployeeToLaptop"
		}
	}
	
	c: Company{
		.name : "Fraunhofer"
	}
	
	e1: Employee{
		.name : "Mario"
	}
	
	e2: Employee{
		.name : "Michael"
	}
}

model InConsistentTarget1{
	it : IT {
		.name : "Fraunhofer"
		-network-> network
	}
	
